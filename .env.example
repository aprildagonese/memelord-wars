# DigitalOcean GradientAI Platform Configuration
# Copy this file to .env and fill in your actual values

# Spicy Agent (with function calling capabilities)
REACT_APP_SPICY_AGENT_ENDPOINT=https://your-spicy-agent-endpoint.com/v1/chat
REACT_APP_SPICY_AGENT_API_KEY=your_spicy_agent_api_key_here

# Classic Agent (timeless humor)
REACT_APP_CLASSIC_AGENT_ENDPOINT=https://your-classic-agent-endpoint.com/v1/chat
REACT_APP_CLASSIC_AGENT_API_KEY=your_classic_agent_api_key_here

# Optional: Function calling endpoints (if you want real APIs instead of mocks)
REACT_APP_TRENDS_API_ENDPOINT=https://api.example.com/trending
REACT_APP_NEWS_API_ENDPOINT=https://api.example.com/news
REACT_APP_STOCK_API_ENDPOINT=https://api.example.com/stocks